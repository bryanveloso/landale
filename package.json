{
  "name": "landale",
  "productName": "Landale",
  "main": "main/index.js",
  "scripts": {
    "clean": "rimraf dist main renderer/out renderer/.next",
    "dev": "npm run build-backend && npm run build-electron && electron .",
    "build-backend": "tsc -p backend",
    "build-renderer": "next build renderer && next export renderer",
    "build-electron": "tsc -p electron",
    "build": "npm run build-backend && npm run build-renderer && npm run build-electron",
    "pack-app": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "type-check": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bryanveloso/landale.git"
  },
  "author": "Bryan Veloso",
  "bugs": {
    "url": "https://github.com/bryanveloso/landale/issues"
  },
  "homepage": "https://github.com/bryanveloso/landale#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.14",
    "eslint": "8.23.0",
    "rimraf": "^3.0.2",
    "turbo": "^1.4.5",
    "typescript": "^4.8.2"
  },
  "build": {
    "appId": "com.landale.app"
  }
}
